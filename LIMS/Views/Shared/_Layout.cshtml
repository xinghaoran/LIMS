<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Full Layout - jQuery EasyUI Demo</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @*<link href="~/Reception/Font-Awesome-3.2.1/css/font-awesome.css" rel="stylesheet" />*@
    <link rel="stylesheet" type="text/css" href="~/Reception/jquery-easyui-1.5/themes/bootstrap/easyui.css">
    <link rel="stylesheet" type="text/css" href="../../Reception/jquery-easyui-1.5/themes/icon.css">
    <link href="~/Content/main.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Reception/jquery-easyui-1.5/jquery.min.js"></script>
    <script type="text/javascript" src="~/Reception/jquery-easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body class="easyui-layout">
    <div data-options="region:'north',border:false" style="height:55px;background:#B3DFDA;">
        <div class="define-head">
            <div class="define-logo">
                <div id="LoginTopLine">胜华泰LIMS——实验室管理系统</div>
                @*<div id="LoginBotoomLine">追求卓越品质</div>*@
            </div>
            @Html.Partial("_LoginPartial")
        </div>
    </div>
    <div data-options="region:'south',border:false" style="height:30px;background:#FFFFFF;padding:10px;">
        <div class="copyrightinfo" style="position: absolute;left: 1%;">
            <span style="font-family:Arial;margin-right:5px;">©</span>2015 北京胜华泰石油检测服务有限责任公司 <a href="http://www.shenghtjc.com/" target="_Blank">官方网站</a>
        </div>
        <div>
            <span id="time" style="position: absolute;right: 2%;"></span>
        </div>
    </div>
    @RenderBody()
</body>
</html>
